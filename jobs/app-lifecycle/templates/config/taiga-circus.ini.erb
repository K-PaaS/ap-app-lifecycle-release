[watcher:taiga]
working_dir = /home/vcap/taiga-back
cmd = gunicorn
args = -w 3 -t 60 --pythonpath=. -b 0.0.0.0:8001 taiga.wsgi
uid = vcap
numprocesses = 1
autostart = true
send_hup = true
stdout_stream.class = FileStream
stdout_stream.filename = /var/vcap/sys/log/app-lifecycle/gunicorn.stdout.log
stdout_stream.max_bytes = 10485760
stdout_stream.backup_count = 3
stderr_stream.class = FileStream
stderr_stream.filename = /var/vcap/sys/log/app-lifecycle/gunicorn.stderr.log
stderr_stream.max_bytes = 10485760
stderr_stream.backup_count = 3

[env:taiga]
PATH = /home/vcap/.virtualenvs/taiga/bin:$PATH
TERM=rxvt-256color
SHELL=/bin/bash
USER=taiga
LANG=en_US.UTF-8
HOME=/home/vcap
PYTHONPATH=/home/vcap/.virtualenvs/taiga/python3.5/site-packages
